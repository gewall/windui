{
    "env": {
        "browser": true,
        "es2021": true
       
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "prettier/prettier": ["error",
            {
                "endOfLine": "auto" 
            }
        ],
        "react/jsx-closing-bracket-location": [1, "tag-aligned"],
        "react/react-in-jsx-scope": "off",
        "react/jsx-filename-extension": [1, {
        "extensions": [".js", ".jsx"]}
        ]}
        

}
